<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="fix" parent="Base.Theme.AppCompat.Light.Dialog.Alert">
		<item name="windowFixedWidthMajor">90%</item>
        <item name="windowFixedWidthMinor">90%</item>
        <item name="android:backgroundDimEnabled">true</item>
	</style>
	<style name="FadeTheme">
		<item name="android:windowEnterAnimation">@android:anim/fade_in</item>
		<item name="android:windowExitAnimation">@android:anim/fade_out</item>
	</style>
	<style name="PopTheme">
        <item name="android:windowEnterAnimation">@anim/pop_in</item>
        <item name="android:windowExitAnimation">@anim/pop_out</item>
    </style>
	<style name="SideTheme">
        <item name="android:windowEnterAnimation">@anim/side_in</item>
        <item name="android:windowExitAnimation">@anim/side_out</item>
    </style>
    <style name="SlideTheme">
        <item name="android:windowEnterAnimation">@anim/slide_in</item>
        <item name="android:windowExitAnimation">@anim/slide_out</item>
    </style>
	<style name="UpTheme">
        <item name="android:windowEnterAnimation">@anim/up_in</item>
        <item name="android:windowExitAnimation">@anim/up_out</item>
    </style>
</resources>
